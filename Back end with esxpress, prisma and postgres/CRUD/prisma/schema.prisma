// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuarios {
  id    Int    @default(autoincrement())
  Email String @unique
  Senha String
  NomeC String
}

model Linhas {
  id    Int    @default(autoincrement())
  Email String @unique
  NomeT String
}
